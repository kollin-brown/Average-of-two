#include <iostream>
#include <string>
using namespace std;
double answer;
int main ()
{
  // Gets the first number from the user.
  int num1;
  cout << "Enter first number:";
  cin >> num1;

  // Gets the second number from the user.
  int num2;
  cout << "Enter second number:";
  cin >> num2;

  // Add your code here.
  answer=num1+num2;
  answer=answer/2;
  cout<<answer;

  return 0;
}
